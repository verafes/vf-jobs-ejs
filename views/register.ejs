<%- include("partials/head.ejs") %>
<%- include("partials/header.ejs") %>
    <form method="POST">
        <!--include CSRF-->
        <input type="hidden" name="_csrf" value="<%= _csrf %>" />
        <div>
            <label name="name">Enter your name:</label>
            <input name="name" /> </div> <div>
            <label name="email">Enter your email:</label>
            <input name="email" /> </div> <div>
            <label name="password">Enter your password:</label>
            <input type="password" name="password" />
        </div>
        <div>
            <label name="confirmPassword">Confirm your password:</label>
            <input type="password" name="confirmPassword" />
        </div>
        <div>
            <button>Register</button>
            <a href="/">
                <button type="button">Cancel</button>
            </a>
        </div>
    </form>
<%- include("partials/footer.ejs") %>